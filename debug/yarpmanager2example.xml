<application>
<name>yarpmanager2example</name>
    <module>
        <name>yarpview</name>
        <parameters>--x 100</parameters>
        <node>localhost</node>
        <prefix>/LeftView</prefix>
        <geometry>(Pos (x 600) (y 104))</geometry>
    </module>
    <module>
        <name>fakeFrameGrabber</name>
        <parameters>--mode line </parameters>
        <node>localhost</node>
        <prefix>/Left</prefix>
        <deployer>yarpdev</deployer>
        <geometry>(Pos (x 25) (y 47.9))</geometry>
    </module>
    <connection>
        <from>/Left/grabber</from>
        <to>/LeftView/yarpview/img:i</to>
        <protocol>udp</protocol>
        <geometry>(Pos ((x 349.5) (y 122)) ((x 286) (y 122)) ((x 413) (y 122))  )</geometry>
    </connection>
</application>
